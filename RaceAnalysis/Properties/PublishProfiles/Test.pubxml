<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://localhost/RaceAnalysis</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>localhost</MSDeployServiceURL>
    <DeployIisAppPath>Default Web Site/RaceAnalysis</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="RaceAnalysis" Order="1" Enabled="True">
          <Destination Path="Data Source=.\SQLExpress;Initial Catalog=RaceAnalysis;Integrated Security=True" Name="Data Source=.\SQLExpress;Initial Catalog=RaceAnalysis;Integrated Security=True;" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="RaceAnalysis.Models.RaceAnalysisDbContext, RaceAnalysis" MigrationConfiguration="RaceAnalysis.Migrations.Configuration, RaceAnalysis" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)RaceAnalysis-Web.config Connection String">
      <ParameterValue>Data Source=.\SQLExpress;Initial Catalog=RaceAnalysis;Integrated Security=True;</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>