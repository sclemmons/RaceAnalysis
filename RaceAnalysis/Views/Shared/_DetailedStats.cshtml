
@model RaceAnalysis.Models.TriStats


   <p/>
    <div>
        <div class="table-responsive">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th></th>
                        <th>Swim</th>
                        <th>Bike</th>
                        <th>Run</th>
                        <th>Finish</th>
                        <th>Div Rank</th>
                        <th>Gender Rank</th>
                        <th>Overall Rank</th>
                        <th>Points</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Fastest:</td>
                        <td>@Html.DisplayFor(s => Model.Swim.Min) </td>
                        <td>@Html.DisplayFor(s => Model.Bike.Min) </td>
                        <td>@Html.DisplayFor(s => Model.Run.Min)</td>
                        <td>@Html.DisplayFor(s => Model.Finish.Min)</td>
                        <td>@Html.DisplayFor(s => Model.DivRank.Min)</td>
                        <td>@Html.DisplayFor(s => Model.GenderRank.Min)</td>
                        <td>@Html.DisplayFor(s => Model.OverallRank.Min)</td>
                        <td>@Html.DisplayFor(s => Model.Points.Max)</td>
                    </tr>
                    <tr>
                        <td>Slowest:</td>
                        <td>@Html.DisplayFor(s => Model.Swim.Max) </td>
                        <td>@Html.DisplayFor(s => Model.Bike.Max) </td>
                        <td>@Html.DisplayFor(s => Model.Run.Max)</td>
                        <td>@Html.DisplayFor(s => Model.Finish.Max)</td>
                        <td>@Html.DisplayFor(s => Model.DivRank.Max)</td>
                        <td>@Html.DisplayFor(s => Model.GenderRank.Max)</td>
                        <td>@Html.DisplayFor(s => Model.OverallRank.Max)</td>
                        <td>@Html.DisplayFor(s => Model.Points.Min)</td>
                    </tr>
                    <tr>
                        <td>Median:</td>
                        <td>@Html.DisplayFor(s => Model.Swim.Median) </td>
                        <td>@Html.DisplayFor(s => Model.Bike.Median) </td>
                        <td>@Html.DisplayFor(s => Model.Run.Median)</td>
                        <td>@Html.DisplayFor(s => Model.Finish.Median)</td>
                        <td>@Html.DisplayFor(s => Model.DivRank.Median)</td>
                        <td>@Html.DisplayFor(s => Model.GenderRank.Median)</td>
                        <td>@Html.DisplayFor(s => Model.OverallRank.Median)</td>
                        <td>@Html.DisplayFor(s => Model.Points.Median)</td>

                    </tr>
                    <tr>
                        <td>Average:</td>
                        <td>@Html.DisplayFor(s => Model.Swim.Average) </td>
                        <td>@Html.DisplayFor(s => Model.Bike.Average) </td>
                        <td>@Html.DisplayFor(s => Model.Run.Average)</td>
                        <td>@Html.DisplayFor(s => Model.Finish.Average)</td>
                        <td>@Html.DisplayFor(s => Model.DivRank.Average)</td>
                        <td>@Html.DisplayFor(s => Model.GenderRank.Average)</td>
                        <td>@Html.DisplayFor(s => Model.OverallRank.Average)</td>
                        <td>@Html.DisplayFor(s => Model.Points.Average)</td>
                    </tr>

                    <tr>
                        <td>Standard Dev:</td>
                        <td>@Html.DisplayFor(s => Model.Swim.StandDev) </td>
                        <td>@Html.DisplayFor(s => Model.Bike.StandDev) </td>
                        <td>@Html.DisplayFor(s => Model.Run.StandDev)</td>
                        <td>@Html.DisplayFor(s => Model.Finish.StandDev)</td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
               </tbody>
            </table>
        <h3> </h3>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th></th>
                        <th>Top 25%</th>
                        <th>Top 50%</th>
                        <th>75% </th>
                        <th>100%</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Swim:</td>
                        <td>@(new TimeSpan(Model.Swim.FastestHalf.Item1.Max(t => t.Swim.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Swim.SlowestHalf.Item1.Min(t => t.Swim.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Swim.SlowestHalf.Item1.Max(t => t.Swim.Ticks))) </td>
                        <td>@Html.DisplayFor(s => Model.Swim.Max)</td>

                    </tr>
                    <tr>
                        <td>Bike:</td>
                        <td>@(new TimeSpan(Model.Bike.FastestHalf.Item1.Max(t => t.Bike.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Bike.SlowestHalf.Item1.Min(t => t.Bike.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Bike.SlowestHalf.Item1.Max(t => t.Bike.Ticks))) </td>
                        <td>@Html.DisplayFor(s => Model.Bike.Max)</td>

                    </tr>
                    <tr>
                        <td>Run:</td>
                        <td>@(new TimeSpan(Model.Run.FastestHalf.Item1.Max(t => t.Run.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Run.SlowestHalf.Item1.Min(t => t.Run.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Run.SlowestHalf.Item1.Max(t => t.Run.Ticks))) </td>
                        <td>@Html.DisplayFor(s => Model.Run.Max)</td>

                    </tr>
                    <tr>
                        <td>Finish:</td>
                        <td>@(new TimeSpan(Model.Finish.FastestHalf.Item1.Max(t => t.Finish.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Finish.SlowestHalf.Item1.Min(t => t.Finish.Ticks))) </td>
                        <td>@(new TimeSpan(Model.Finish.SlowestHalf.Item1.Max(t => t.Finish.Ticks))) </td>
                        <td>@Html.DisplayFor(s => Model.Finish.Max)</td>
                    </tr>

                </tbody>
            </table>
        </div> <!--responsive-->
     </div> <!--container-->


