@model RaceAnalysis.Models.KeyValueViewModel

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Key</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var kvp in Model.KeyValuePairs)
            {
                <tr>
                    <td>
                        @kvp.Key
                    </td>
                    <td>
                        @kvp.Value

                    </td>
                </tr>
            }

           

        </tbody>
    </table>
</div>
