@model RaceAnalysis.Models.TriathletesCompareViewModel

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<div>
    <div id='UniqueId' style="width: 600px; height: 500px;"></div>
    <script type="text/javascript">

        google.charts.load('current', { 'packages': ['corechart'] });

            google.charts.setOnLoadCallback(function () {

            var data = new google.visualization.DataTable(
                                @Html.Raw(Json.Encode(Model.ChartData)))
            
              var options = {
                width: 600,
                height: 400,
                legend: { position: 'right'},
                bar: { groupWidth: '50%' },
                isStacked: true
              };
            
            var chart = new google.visualization.BarChart(document.getElementById('UniqueId'));
            chart.draw(data, options);
            });
    </script>
</div>